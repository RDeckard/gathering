<p id="notice"><%= notice %></p>

<h1>Workshop Templates</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @workshop_templates.each do |workshop_template| %>
      <tr>
        <td><%= workshop_template.name %></td>
        <td><%= workshop_template.description %></td>
        <td><%= link_to 'Show', workshop_template %></td>
        <td><%= link_to 'Edit', edit_workshop_template_path(workshop_template) %></td>
        <td><%= link_to 'Destroy', workshop_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workshop Template', new_workshop_template_path %>
